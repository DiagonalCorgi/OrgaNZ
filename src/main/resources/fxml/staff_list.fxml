<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0"
      prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="seng302.Controller.Administrator.StaffListController">
    <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
    <HBox fx:id="menuBarPane" maxHeight="-Infinity" minHeight="-Infinity" />
   <SplitPane dividerPositions="0.5" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
       <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
           <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Staff list" VBox.vgrow="ALWAYS">
              <font>
                  <Font size="24.0"/>
              </font>
              <cursor>
                  <Cursor fx:constant="NONE"/>
              </cursor>
              <VBox.margin>
                  <Insets bottom="10.0" top="50.0"/>
              </VBox.margin>
           </Text>
           <ListView fx:id="staffList" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
              <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
              </VBox.margin>
           </ListView>
       </VBox>
   </SplitPane>
    </SplitPane>

</VBox>
