<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.Controller.HistoryController">
    <children>
        <SplitPane dividerPositions="0.5" VBox.vgrow="ALWAYS">
            <items>
                <HBox fx:id="sidebarPane" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
                <VBox prefHeight="600.0" prefWidth="800.0">
                    <children>
                        <BorderPane VBox.vgrow="ALWAYS">
                            <top>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="History" textAlignment="CENTER" BorderPane.alignment="CENTER">
                                    <font>
                                        <Font size="24.0" />
                                    </font>
                                    <BorderPane.margin>
                                        <Insets top="50.0" />
                                    </BorderPane.margin>
                                </Text>
                            </top>
                     <center>
                        <TableView fx:id="historyTable" prefHeight="520.0" prefWidth="565.0" BorderPane.alignment="CENTER">
                          <columns>
                            <TableColumn fx:id="timeCol" prefWidth="166.0" text="Time" />
                              <TableColumn fx:id="typeCol" prefWidth="152.0" text="Type" />
                            <TableColumn fx:id="commandCol" maxWidth="1.7976931348623157E308" prefWidth="500.0" text="Command " />
                          </columns>
                        </TableView>
                     </center>
                        </BorderPane>
                    </children>
                </VBox>
            </items>
        </SplitPane>
    </children>
</VBox>
