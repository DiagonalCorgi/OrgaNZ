   <children>
      <SplitPane dividerPositions="0.298" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <items>
            <HBox fx:id="sidebarPane" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
            <BorderPane>
               <center>
                  <TableView fx:id="tableView">
                    <columns>
                      <TableColumn fx:id="idCol" editable="false" prefWidth="75.0" text="ID" />
                        <TableColumn fx:id="nameCol" editable="false" prefWidth="261.0" text="Name" />
                        <TableColumn fx:id="ageCol" editable="false" prefWidth="64.0" text="Age" />
                        <TableColumn fx:id="genderCol" editable="false" prefWidth="99.0" text="Gender" />
                        <TableColumn fx:id="regionCol" editable="false" prefWidth="100.0" text="Region" />
                    </columns>
                     <BorderPane.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                     </BorderPane.margin>
                  </TableView>
               </center>
               <bottom>
                  <Pagination fx:id="pagination" prefHeight="70.0" prefWidth="200.0" BorderPane.alignment="BOTTOM_CENTER" />
               </bottom>
               <top>
                  <TextField fx:id="searchBox" promptText="Donor name" BorderPane.alignment="CENTER">
                     <BorderPane.margin>
                        <Insets left="100.0" right="100.0" top="20.0" />
                     </BorderPane.margin>
                  </TextField>
               </top>
            </BorderPane>
         </items>
      </SplitPane>
   </children>
</VBox>
