<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.Controller.MainController">
   <Pane fx:id="CreateUserPane" prefHeight="400.0" prefWidth="600.0" />
    <Pane fx:id="InitialPane" prefHeight="400.0" prefWidth="600.0">
        <Button fx:id="createUserBtn" layoutX="108.0" layoutY="238.0" mnemonicParsing="false"
                onAction="#viewCreateUserPane" text="Create User"/>
        <Button fx:id="loginBtn" layoutX="343.0" layoutY="225.0" mnemonicParsing="false" text="Login"/>
        <Label fx:id="initialLbl" layoutX="108.0" layoutY="72.0" prefHeight="74.0" prefWidth="258.0"
               text="Hello There! For new users, please click on 'Create User'. For existing users, please select 'Login'"
               wrapText="true"/>
    </Pane>
</AnchorPane>
