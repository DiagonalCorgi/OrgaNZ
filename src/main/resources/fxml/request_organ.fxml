<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="seng302.Controller.Client.RequestOrganController">
  <children>
    <SplitPane dividerPositions="0.5" VBox.vgrow="ALWAYS">
      <items>
        <HBox fx:id="sidebarPane" maxWidth="200.0" minWidth="200.0" prefWidth="200.0"/>
        <VBox prefHeight="600.0" prefWidth="800.0">
          <children>
            <BorderPane VBox.vgrow="ALWAYS">
              <top>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Current Requested Organs" textAlignment="CENTER"
                  BorderPane.alignment="CENTER">
                  <font>
                    <Font size="24.0"/>
                  </font>
                  <BorderPane.margin>
                    <Insets top="50.0"/>
                  </BorderPane.margin>
                </Text>
              </top>
              <center>
                <VBox BorderPane.alignment="CENTER">
                  <BorderPane.margin>
                    <Insets bottom="40.0" left="100.0" right="100.0"/>
                  </BorderPane.margin>
                  <children>
                    <GridPane fx:id="idPane" hgap="50.0" layoutX="10.0" layoutY="10.0">
                      <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                      </columnConstraints>
                      <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                      </rowConstraints>
                      <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Organs requested for User ID">
                          <font>
                            <Font size="14.0"/>
                          </font>
                        </Text>
                        <TextField fx:id="fieldUserID" onAction="#updateUserID" promptText="User ID"
                          GridPane.columnIndex="1"/>
                      </children>
                      <VBox.margin>
                        <Insets bottom="15.0" top="15.0"/>
                      </VBox.margin>
                    </GridPane>
                    <GridPane hgap="50.0" VBox.vgrow="ALWAYS">
                      <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                      </columnConstraints>
                      <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                      </rowConstraints>
                      <children>
                        <CheckBox fx:id="checkBoxLiver" mnemonicParsing="false" onAction="#modifyRequests"
                          text="Liver"/>
                        <CheckBox fx:id="checkBoxKidney" layoutX="10.0" layoutY="127.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Kidney" GridPane.rowIndex="1"/>
                        <CheckBox fx:id="checkBoxPancreas" layoutX="10.0" layoutY="43.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Pancreas" GridPane.rowIndex="2"/>
                        <CheckBox fx:id="checkBoxHeart" layoutX="10.0" layoutY="295.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Heart" GridPane.rowIndex="3"/>
                        <CheckBox fx:id="checkBoxLung" layoutX="10.0" layoutY="379.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Lung" GridPane.rowIndex="4"/>
                        <CheckBox fx:id="checkBoxIntestine" layoutX="10.0" layoutY="463.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Intestine" GridPane.rowIndex="5"/>
                        <CheckBox fx:id="checkBoxCornea" layoutX="10.0" layoutY="547.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Cornea" GridPane.columnIndex="1"/>
                        <CheckBox fx:id="checkBoxMiddleEar" layoutX="535.0" layoutY="127.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Middle Ear" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                        <CheckBox fx:id="checkBoxSkin" layoutX="535.0" layoutY="211.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Skin" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                        <CheckBox fx:id="checkBoxBone" layoutX="535.0" layoutY="295.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Bone" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                        <CheckBox fx:id="checkBoxBoneMarrow" layoutX="535.0" layoutY="379.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Bone Marrow" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                        <CheckBox fx:id="checkBoxConnTissue" layoutX="535.0" layoutY="463.0" mnemonicParsing="false"
                          onAction="#modifyRequests" text="Connective Tissue" GridPane.columnIndex="1"
                          GridPane.rowIndex="5"/>
                        <Button fx:id="requestHistoryButton" mnemonicParsing="false" onAction="#viewRequestHistory"
                          prefHeight="34.0" prefWidth="170.0" text="Request History" GridPane.columnIndex="1"
                          GridPane.rowIndex="6"/>
                      </children>
                    </GridPane>
                  </children>
                </VBox>
              </center>
            </BorderPane>
          </children>
        </VBox>
      </items>
    </SplitPane>
  </children>
</VBox>
