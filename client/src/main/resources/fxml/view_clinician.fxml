<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.121"
  fx:controller="com.humanharvest.organz.controller.clinician.ViewClinicianController">
  <SplitPane VBox.vgrow="ALWAYS" dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0">
    <HBox maxHeight="20.0" minHeight="-Infinity" fx:id="menuBarPane"/>
    <SplitPane VBox.vgrow="ALWAYS">
      <VBox prefHeight="600.0" prefWidth="800.0">
        <BorderPane VBox.vgrow="ALWAYS">
          <center>
            <VBox BorderPane.alignment="CENTER" prefHeight="403.0" prefWidth="798.0">
              <BorderPane.margin>
                <Insets bottom="40.0" left="100.0" right="100.0"/>
              </BorderPane.margin>
              <GridPane VBox.vgrow="ALWAYS" hgap="30.0" prefHeight="302.0" prefWidth="758.0"
                fx:id="inputsPane">
                <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"
                    minWidth="10.0" prefWidth="100.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"
                    minWidth="10.0" prefWidth="126.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"
                    minWidth="10.0" prefWidth="99.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"
                    minWidth="10.0" prefWidth="149.0"/>
                </columnConstraints>
                <rowConstraints>
                  <RowConstraints minHeight="-Infinity" prefHeight="45.0"/>
                  <RowConstraints minHeight="-Infinity" prefHeight="45.0"/>
                  <RowConstraints minHeight="-Infinity" prefHeight="45.0"/>
                  <RowConstraints minHeight="-Infinity" prefHeight="45.0"/>
                  <RowConstraints minHeight="-Infinity" prefHeight="45.0"/>
                  <RowConstraints minHeight="-Infinity" prefHeight="45.0"/>
                </rowConstraints>
                <children>
                  <TextField GridPane.columnIndex="1" layoutX="122.0" layoutY="80.0" prefHeight="30.0"
                    fx:id="fname"/>
                  <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" layoutX="122.0" layoutY="191.0"
                    prefHeight="30.0" fx:id="mname"/>
                  <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" layoutX="122.0" layoutY="136.0"
                    prefHeight="30.0" fx:id="lname"/>
                  <TextField GridPane.columnIndex="3" GridPane.rowIndex="1" prefHeight="30.0"
                    fx:id="workAddress"/>
                  <ChoiceBox GridPane.columnIndex="3" GridPane.rowIndex="2" maxWidth="1.7976931348623157E308"
                    prefHeight="30.0" fx:id="regionCB"/>
                  <Label GridPane.columnIndex="2" GridPane.rowIndex="4" layoutX="41.0"
                    text="Profile created on:"/>
                  <Label layoutY="85.0" text="First name:" fx:id="fnameLabel"/>
                  <Label GridPane.rowIndex="1" layoutY="196.0" text="Middle name:"
                    fx:id="mnameLabel"/>
                  <Label GridPane.rowIndex="2" layoutY="141.0" text="Last name:" fx:id="lnameLabel"/>
                  <Label GridPane.columnIndex="2" GridPane.rowIndex="1" layoutX="1.0" layoutY="250.0"
                    text="Work address:" fx:id="workAddressLabel"/>
                  <Label GridPane.columnIndex="2" GridPane.rowIndex="5" layoutX="446.0"
                    text="Last modified:"/>
                  <Label GridPane.columnIndex="2" GridPane.rowIndex="2" layoutX="423.0" layoutY="196.0"
                    text="Region:" fx:id="regionLabel"/>
                  <Label GridPane.columnIndex="2" layoutX="419.0" layoutY="250.0" text="Password:"
                    fx:id="passwordLabel"/>
                  <Label GridPane.columnIndex="3" GridPane.rowIndex="4" layoutX="146.0" text="DATE"
                    wrapText="true" fx:id="creationDate">
                    <font>
                      <Font name="System Italic" size="13.0"/>
                    </font>
                  </Label>
                  <Label GridPane.columnIndex="3" GridPane.rowIndex="5" layoutX="539.0" text="DATE"
                    wrapText="true" fx:id="lastModified">
                    <font>
                      <Font name="System Italic" size="13.0"/>
                    </font>
                  </Label>
                  <PasswordField GridPane.columnIndex="3" prefHeight="30.0" fx:id="password"/>
                  <Label GridPane.columnIndex="2" GridPane.rowIndex="3" text="Country"/>
                  <ChoiceBox GridPane.columnIndex="3" GridPane.rowIndex="3" onAction="#checkCountry"
                    prefWidth="150.0"
                    fx:id="country"/>
                  <TextField GridPane.columnIndex="3" GridPane.rowIndex="2" fx:id="regionTF"/>
                  <Label GridPane.rowIndex="3" text="Staff ID"/>
                  <Label GridPane.columnIndex="1" GridPane.rowIndex="3" text="STAFF ID"
                    fx:id="staffIdLabel"/>
                </children>
                <VBox.margin>
                  <Insets/>
                </VBox.margin>
                <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="40.0"/>
                </padding>
              </GridPane>
            </VBox>
          </center>
          <top>
            <Label BorderPane.alignment="CENTER" text="Clinician profile" fx:id="title">
              <font>
                <Font size="24.0"/>
              </font>
              <BorderPane.margin>
                <Insets top="50.0"/>
              </BorderPane.margin>
            </Label>
          </top>
        </BorderPane>
      </VBox>
    </SplitPane>
  </SplitPane>
  <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="200.0">
    <children>
      <Button cancelButton="true" mnemonicParsing="false" onAction="#cancel" prefHeight="30.0"
        prefWidth="100.0" text="Cancel" fx:id="cancelButton">
        <HBox.margin>
          <Insets/>
        </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#apply" prefHeight="30.0" prefWidth="100.0" text="Apply"
        fx:id="applyButton">
        <HBox.margin>
          <Insets left="20.0" right="20.0"/>
        </HBox.margin>
      </Button>
    </children>
    <opaqueInsets>
      <Insets/>
    </opaqueInsets>
  </HBox>
</VBox>
