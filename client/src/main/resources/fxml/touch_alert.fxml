<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0"
  prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121"
  fx:controller="com.humanharvest.organz.controller.components.TouchAlertController">
  <StackPane VBox.vgrow="ALWAYS" fx:id="pageHolder">
    <BorderPane>
      <center>
        <Text BorderPane.alignment="CENTER" strokeType="OUTSIDE" strokeWidth="0.0" text="Body" fx:id="body"/>
      </center>
      <bottom>
        <HBox BorderPane.alignment="CENTER_RIGHT" alignment="CENTER_RIGHT" fillHeight="false" spacing="15.0">
          <children>
            <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" fx:id="cancelButton"/>
            <Button mnemonicParsing="false" onAction="#ok" text="OK"/>
          </children>
        </HBox>
      </bottom>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <top>
        <HBox BorderPane.alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
          <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Title" fx:id="title"/>
          </children>
          <BorderPane.margin>
            <Insets/>
          </BorderPane.margin>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
          </padding>
        </HBox>
      </top>
    </BorderPane>
  </StackPane>
</Pane>
