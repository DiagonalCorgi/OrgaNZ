<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0"
      prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="com.humanharvest.organz.controller.components.TouchAlertController">
    <StackPane fx:id="pageHolder" VBox.vgrow="ALWAYS">
        <BorderPane>
            <center>
                <Text fx:id="body" strokeType="OUTSIDE" strokeWidth="0.0" text="Body" BorderPane.alignment="CENTER"/>
            </center>
            <bottom>
                <HBox alignment="CENTER_RIGHT" fillHeight="false" spacing="15.0" BorderPane.alignment="CENTER_RIGHT">
                    <children>
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="Cancel"/>
                        <Button mnemonicParsing="false" onAction="#ok" text="OK"/>
                    </children>
                </HBox>
            </bottom>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <top>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="50.0" BorderPane.alignment="TOP_RIGHT">
                    <children>
                        <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0" text="Title"/>
                    </children>
                    <BorderPane.margin>
                        <Insets/>
                    </BorderPane.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </HBox>
            </top>
        </BorderPane>
    </StackPane>
</Pane>
