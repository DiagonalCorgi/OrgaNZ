<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.humanharvest.organz.controller.components.TouchAlertController">
  <StackPane fx:id="pageHolder" maxHeight="200.0" maxWidth="300.0" VBox.vgrow="ALWAYS">
    <BorderPane maxHeight="200.0" maxWidth="300.0" prefHeight="200.0" prefWidth="300.0">
      <bottom>
        <HBox alignment="CENTER_RIGHT" fillHeight="false" spacing="15.0" BorderPane.alignment="CENTER_RIGHT">
          <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
            <Button mnemonicParsing="false" onAction="#ok" text="OK" />
          </children>
        </HBox>
      </bottom>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      <top>
        <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" BorderPane.alignment="CENTER">
          <font>
            <Font name="System Bold" size="15.0" />
          </font>
        </Text>
      </top>
      <center>
        <Text fx:id="body" strokeType="OUTSIDE" strokeWidth="0.0" text="Body" wrappingWidth="280.0" BorderPane.alignment="CENTER" />
      </center>
    </BorderPane>
  </StackPane>
</Pane>