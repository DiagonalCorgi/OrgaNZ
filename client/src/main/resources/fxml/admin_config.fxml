<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.CheckComboBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.151"
  fx:controller="com.humanharvest.organz.controller.ConfigController">
  <children>
    <SplitPane VBox.vgrow="ALWAYS" dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0"
      prefWidth="200.0">
      <items>
        <HBox maxHeight="20.0" minHeight="-Infinity" fx:id="menuBarPane"/>
        <SplitPane VBox.vgrow="ALWAYS">
          <items>
            <VBox prefHeight="600.0" prefWidth="800.0">
              <children>
                <BorderPane VBox.vgrow="ALWAYS">
                  <center>
                    <VBox BorderPane.alignment="CENTER">
                      <BorderPane.margin>
                        <Insets bottom="40.0" left="100.0" right="100.0"/>
                      </BorderPane.margin>
                      <children>
                        <GridPane prefHeight="118.0" prefWidth="798.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                              prefWidth="100.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                              prefWidth="100.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                              prefWidth="100.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                              prefWidth="100.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                              prefWidth="100.0"/>
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0"
                              vgrow="SOMETIMES"/>
                          </rowConstraints>
                          <children>
                            <Label GridPane.columnIndex="1" prefHeight="15.0"
                              prefWidth="180.0" text="Allowed countries:"/>
                            <GridPane GridPane.columnIndex="3" vgap="10.0">
                              <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                  prefWidth="100.0"/>
                              </columnConstraints>
                              <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                  vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                  vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                  vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                  vgrow="SOMETIMES"/>
                              </rowConstraints>
                              <children>
                                <Button GridPane.halignment="CENTER" GridPane.rowIndex="1"
                                  GridPane.valignment="CENTER"
                                  mnemonicParsing="false"
                                  onAction="#selectAll"
                                  text="Select All    "/>
                                <Button GridPane.halignment="CENTER" GridPane.rowIndex="2"
                                  GridPane.valignment="CENTER" mnemonicParsing="false"
                                  onAction="#selectNone"
                                  text="Select None"/>
                              </children>
                              <GridPane.margin>
                                <Insets/>
                              </GridPane.margin>
                            </GridPane>
                            <CheckComboBox GridPane.columnIndex="2" prefHeight="26.0"
                              prefWidth="174.0" fx:id="countries"/>
                          </children>
                        </GridPane>
                      </children>
                    </VBox>
                  </center>
                  <top>
                    <Label BorderPane.alignment="CENTER" prefHeight="44.0" prefWidth="176.0"
                      text="Configurations">
                      <font>
                        <Font size="24.0"/>
                      </font>
                      <BorderPane.margin>
                        <Insets top="50.0"/>
                      </BorderPane.margin>
                    </Label>
                  </top>
                </BorderPane>
              </children>
            </VBox>
          </items>
        </SplitPane>
      </items>
    </SplitPane>
    <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="200.0">
      <children>
        <Button cancelButton="true" mnemonicParsing="false" onAction="#cancel" prefHeight="30.0"
          prefWidth="100.0" text="Cancel">
          <HBox.margin>
            <Insets/>
          </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#apply" prefHeight="30.0" prefWidth="100.0" text="Apply">
          <HBox.margin>
            <Insets left="20.0" right="20.0"/>
          </HBox.margin>
        </Button>
      </children>
      <opaqueInsets>
        <Insets/>
      </opaqueInsets>
    </HBox>
  </children>
</VBox>
