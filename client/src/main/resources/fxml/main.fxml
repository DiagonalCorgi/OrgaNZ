<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import com.jfoenix.controls.JFXHamburger?>
<?import com.jfoenix.controls.JFXDrawer?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.121"
  fx:controller="com.humanharvest.organz.controller.MainController">
  <children>
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
      AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS">
      <children>
        <StackPane VBox.vgrow="ALWAYS" fx:id="pageHolder"/>
      </children>
    </VBox>
    <JFXDrawer fx:id="drawer" defaultDrawerSize="230.0">

    </JFXDrawer>
    <JFXHamburger fx:id="hamburger" onMouseClicked="#triggerDrawer">
    </JFXHamburger>
  </children>
</AnchorPane>
