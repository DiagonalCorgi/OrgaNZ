<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="400.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="320.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" fx:controller="com.humanharvest.organz.controller.client.DeceasedDonorOverviewController">
  <children>
    <ImageView fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" fx:id="imageView"/>
    <VBox prefWidth="200.0">
      <children>
        <Label maxWidth="1.7976931348623157E308" text="Name" fx:id="nameLabel">
          <VBox.margin>
            <Insets/>
          </VBox.margin>
          <font>
            <Font size="19.0"/>
          </font>
        </Label>
        <GridPane VBox.vgrow="ALWAYS" maxHeight="1.7976931348623157E308">
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES"/>
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="25.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="25.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="25.0" vgrow="SOMETIMES"/>
          </rowConstraints>
          <children>
            <Label maxWidth="1.7976931348623157E308" text="Time of Death" wrapText="true" fx:id="timeOfDeathLabel"/>
            <Label GridPane.rowIndex="1" maxWidth="1.7976931348623157E308" text="Hospital" wrapText="true" fx:id="hospitalLabel"/>
            <Label GridPane.rowIndex="2" maxWidth="1.7976931348623157E308" text="Number of Organs" wrapText="true" fx:id="numOrgansLabel"/>
          </children>
        </GridPane>
      </children>
      <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
      </padding>
    </VBox>
  </children>
</VBox>
